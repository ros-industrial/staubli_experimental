<?xml version="1.0" ?>
<robot name="staubli_tx90l_gazebo" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- TX90L -->
  <xacro:include filename="$(find staubli_tx90_gazebo)/urdf/tx90l_macro.xacro"/>	
  
  <xacro:staubli_tx90l_robot prefix=""/>

  <!-- Fix robot to Gazebo world -->
  <link name="world"/>
  <joint name="world-base_link-fixed" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0" />
  </joint>
</robot>
